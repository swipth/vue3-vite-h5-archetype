{
  "name": "{{projectName}}",
  "private": true,
  "version": "{{version}}",
  "type": "module",
  "author": "{{author}}",
  "description": "{{description}}",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
    "build:test": "vue-tsc && env-cmd -f .env.test vite build && node version/update.cjs test",
    "build:prod": "vue-tsc && env-cmd -f .env.production vite build && node version/update.cjs production",
    "lint:eslint": "eslint {src,mock}/**/*.{vue,js,ts,tsx} --fix",
    "lint:stylelint": "stylelint {src,mock}/**/*.{css,less,sass,scss,vue} --fix",
    "lint:prettier": "prettier --write \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "watch:i18n": "node watchI18n.cjs",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mitojs/browser": "^3.0.1",
    "@mitojs/vue": "^3.0.1",
    "axios": "^1.7.2",
    "js-base64": "^3.7.2",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "qrcode": "^1.5.1",
    "qs": "^6.10.3",
    "socket.io-client": "^4.5.0",
    "store": "^2.0.12",
    "uuid": "^9.0.0",
    "vant": "^4.8.0",
    "vue": "^3.3.11",
    "vue-i18n": "^9.8.0",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.0",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.7.0",
    "husky": "^8.0.3",
    "less": "^4.1.2",
    "less-loader": "^6.2.0",
    "lint-staged": "^13.0.3",
    "ora": "^5.4.1",
    "postcss-html": "^1.5.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^3.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^4.9.5",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^5.4.0",
    "vite-plugin-eslint": "^1.6.1",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^1.8.25"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npm run lint",
      "git add .",
      "git commit"
    ]
  }
}
